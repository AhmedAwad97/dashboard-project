              <footer class="mt-15 fs-12 c-grey flex-between align-c">
                <span>11/06/2023</span>
                <span>2.8MB</span>
              </footer>
            </div>
            <!--End Card-->
            <!--Start Card-->
            <div class="card bg-white p-relative rad-10 pad-20">
              <a href="#">
                <i class="fa-solid fa-download fs-16 c-grey"></i>
              </a>
              <header>
                <div class="txt-c">
                  <img src="img/dll.svg" alt="psd" />
                  <p class="fs-16 d-block mt-15 mb-10">my-file.dll</p>
                </div>
                <span class="c-grey fs-12 d-block mb-10">Ahmed</span>
              </header>
              <footer class="mt-15 fs-12 c-grey flex-between align-c">
                <span>11/06/2023</span>
                <span>2.8MB</span>
              </footer>
            </div>
            <!--End Card-->
            <!--Start Card-->
            <div class="card bg-white p-relative rad-10 pad-20">
              <a href="#">
                <i class="fa-solid fa-download fs-16 c-grey"></i>
              </a>
              <header>
                <div class="txt-c">
                  <img src="img/pdf.svg" alt="pdf" />
                  <p class="fs-16 d-block mt-15 mb-10">my-file.pdf</p>
