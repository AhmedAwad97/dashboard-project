            <!--End Card-->
          </div>
          <!--End Cards-->

          <!--Start File Statistics-->
          <div class="file-statistics bg-white rad-6 pad-20">
            <h2 class="fs-14 mb-20">Files Statistics</h2>
            <div class="row pdf flex-between align-c pad-10 rad-6">
              <i class="fa-regular fa-file-pdf"></i>
              <div class="info">
                <span class="d-block">DPF Files</span>
                <span class="d-block c-grey">130</span>
              </div>
              <span class="c-grey">6.5GB</span>
            </div>

            <div class="row images flex-between align-c pad-10 rad-6">
              <i class="fa-regular fa-image"></i>
              <div class="info">
                <span class="d-block">Images</span>
                <span class="d-block c-grey">12</span>
              </div>
              <span class="c-grey">25MB</span>
            </div>

            <div class="row word flex-between align-c pad-10 rad-6">
              <i class="fa-regular fa-file-word"></i>
              <div class="info">
                <span class="d-block">Word Files</span>
                <span class="d-block c-grey">12</span>
              </div>
              <span class="c-grey">25MB</span>
            </div>

            <div class="row csv flex-between align-c pad-10 rad-6">
              <i class="fa-solid fa-file-csv"></i>
              <div class="info">
                <span class="d-block">CSV Files</span>
                <span class="d-block c-grey">45</span>
              </div>
              <span class="c-grey">255MB</span>
            </div>
            <a href="#" class="btn-shape bg-blue c-white rad-6 fs-14">
              <i class="fa-solid fa-angles-up mr-10"></i>
              Upload
            </a>
          </div>
          <!--End File Statistics-->
        </div>
      </div>
      <!--End Content-->
    </div>
    <script src="JS/main.js"></script>
